      <div class="row my-4">
        <div class="col-lg-12 col-md-12 mb-md-0 mb-4">
          <div class="card">
            <div class="card-header pb-0">
              <div class="row">
                <div class="col-lg-6 col-7">
                  <h6>All contacts fetchs</h6>
                  <p class="text-sm mb-0">
                    <i class="fa fa-check text-info" aria-hidden="true"></i>
                    <span class="font-weight-bold ms-1"> &nbsp; <span id="contactCount" class="font-weight-bolder mb-0 mt-3 fs-6 text-danger">
                        Loading...
                    </span>&nbsp;&nbsp; contact count </span> this month
                  </p>
                </div>
                <div class="col-lg-6 col-5 my-auto text-end">
                  <div class="dropdown float-lg-end pe-4">
                    <a class="cursor-pointer" id="dropdownTable" data-bs-toggle="dropdown" aria-expanded="false">
                      <i class="fa fa-ellipsis-v text-secondary"></i>
                    </a>
                    <ul class="dropdown-menu px-2 py-3 ms-sm-n4 ms-n5" aria-labelledby="dropdownTable">
                      <li><a class="dropdown-item border-radius-md" href="javascript:;">Action</a></li>
                      <li><a class="dropdown-item border-radius-md" href="javascript:;">Another action</a></li>
                      <li><a class="dropdown-item border-radius-md" href="javascript:;">Something else here</a></li>
                    </ul>
                  </div>
                </div>
              </div>
            </div>
            <div class="card-body px-4 pb-4" style="color: black;">
              <% if (success_msg) { %>
                <div class="alert alert-success text-white w-100">
                  <%= success_msg %>
                </div>
                <% } %>
                  <% if (error_msg) { %>
                    <div class="alert alert-danger text-white w-100">
                      <%= error_msg %>
                    </div>
                    <% } %>
              <table style="width: 100%; border-collapse: collapse; text-align: left; color: black;">
                <thead>
                  <tr>
                    <th style="border: 1px solid #ccc; padding: 8px;">Name</th>
                    <th style="border: 1px solid #ccc; padding: 8px;">Email</th>
                    <th style="border: 1px solid #ccc; padding: 8px;">Subject</th>
                    <th style="border: 1px solid #ccc; padding: 8px;">Message</th>
                    <th style="border: 1px solid #ccc; padding: 8px;">Emergency Type</th>
                    <th style="border: 1px solid #ccc; padding: 8px;">Date</th>
                    <th style="border: 1px solid #ccc; padding: 8px;">Action</th>
                  </tr>
                </thead>
                <tbody>
                  <% contacts.forEach(contact => { %>
                    <tr>
                      <td style="border: 1px solid #ccc; padding: 8px;"><%= contact.name %></td>
                      <td style="border: 1px solid #ccc; padding: 8px;"><%= contact.email %></td>
                      <td style="border: 1px solid #ccc; padding: 8px;"><%= contact.subject %></td>
