<!-- Submit Manuscript Section -->
<section id="submit-manuscript" class="submit-manuscript section light-background">
    <div class="container" data-aos="fade-up" data-aos-delay="100">
        <div class="row justify-content-center">
            <div class="col-lg-10 col-md-12">
                <div class="submit-form" data-aos="fade-up" data-aos-delay="300">
                    <h2>Submit Your Manuscript</h2>
                    <p>Publish your research with JournalsLibrary and engage with global scientific minds. Experience a streamlined submission process with us.</p>

                    <form action="/submitmanuscripts" method="POST" enctype="multipart/form-data">
                        <div class="row gy-4">
                            <!-- Corresponding Author's Name -->
                            <div class="col-md-6">
                                <input type="text" name="author_name" class="form-control danger" placeholder="Name of the Corresponding Author *" required>
                            </div>

                            <!-- Title of the Paper -->
                            <div class="col-md-6">
                                <input type="text" name="paper_title" class="form-control danger" placeholder="Title of the Paper *" required>
                            </div>

                            <!-- Email -->
                            <div class="col-12">
                                <input type="email" class="form-control danger" name="email" placeholder="Email *" required>
                            </div>

                            <!-- Country -->
                            <div class="col-12">
                                <input type="text" class="form-control" name="country" placeholder="Country" required>
                            </div>

                            <!-- Upload Paper -->
                            <div class="col-12">
                                <label for="paper_upload" class="form-label danger">Upload Paper (DOC/DOCX only) *</label>
                                <input type="file" class="form-control" id="paper_upload" name="paper_file" accept=".doc, .docx, .pdf" required>
                            </div>
                            <div class="col-12">
                              <input type="checkbox" name="" required> Consent to JournalsLibrary.net service <a href="#">policy</a>.
                            </div>
                              <!-- Submit Button -->
                            <div class="col-12 text-center">
                              <button type="submit" class="btn btn-primary w-100">Submit Manuscript</button>
                          </div>
                            <!-- Assistance Message -->
                            <div class="col-12 text-center">
                                <p class="help-text">Having trouble uploading your paper? Donâ€™t worry. You can also submit your paper via email at <a href="mailto:editor@journalslibrary.net">editor@journalslibrary.net</a></p>
                            </div>

                            
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</section><!-- /Submit Manuscript Section -->

